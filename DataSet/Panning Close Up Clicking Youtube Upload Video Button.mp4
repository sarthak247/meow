<script>
                function serverProcessText(inputText) {
                    // set text to server
                    var result = inputText;
                    jQuery.ajax({
                        url: '/app/word_wrapper.php?source_text=' + inputText,
                        success: function(data) {
                            result = data
                        },
                        async: false
                    });
                    return result;
                }

                function websiteSearch2(term) {
                    keyword = $(".searchinput2").val();
                    if (typeof keyword == "undefined") {
                        keyword = $('.search-field').val();
                    }

                    if (term) {
                        keyword = term;
                    }

                    promo = '/';
                    if (keyword.substring(0, 1) == '#') {
                        promo += 'promo';
                        keyword = keyword.substring(1);
                    }

                    var oldTimestamp = localStorage.getItem('showedPopup');
                    var difference = 86400000; //2629743.83;

                    var currentTimestamp = Date.now();

                    var timeout = 1000;
                    if ((oldTimestamp == null || currentTimestamp - oldTimestamp > difference) && $("#account_type_input").val() == 0) {
                        try {
                            localStorage.setItem('showedPopup', currentTimestamp);
                        } catch (error) {
                            console.log(error);
                        }
                        keyword = serverProcessText(keyword);
                        var strWord = keyword.replace(/%20/g, "+");
                        var strWord2 = strWord.replace(/ /g, "+");

                        url = 'https://shutterstock.7eer.net/c/51471/43068/1305?u=https://www.shutterstock.com/video/search?searchterm=' + strWord2;

                        if (keyword == 'shutterstock-popular') {
                            url = 'https://shutterstock.7eer.net/c/51471/43068/1305?u=https://www.shutterstock.com/video/search/';
                        }

                        // GA track
                        ga('send', 'event', {
                            eventCategory: 'Shutterstock Redirect',
                            eventAction: 'Opened websiteSearch2 pagejs',
                            eventLabel: 'url: ' + url
                        });
                        // disable shutterstock popup on search
                        // window.open(url, 'Shutterstock', "height=480,width=640");
                        window.open(url); // Shutterstock Popup in new tab
                        timeout = 1000;
                    }

                    var clip_type = "";
                    var clip_array = {
                        "Footage": "free-stock-footage",
                        "Motion Graphics": "free-motion-graphics",
                        "Music": "royalty-free-music",
                        "Sound Effects": "royalty-free-sound-effects"
                    };
                    if ($(".search-category-filter .category-selector span.text").length) {
                        if (!($(".search-category-filter .category-selector span.text").text().localeCompare("All") == 0))
                            clip_type = '/clip_type/' + clip_array[$(".search-category-filter .category-selector span.text").text()];
                    } else {
                        clip_type = "";
                    }

                    if (!term) {
                        setTimeout(function() {

                            window.location = 'https://videvo.net/search/' + keyword + clip_type + promo;
                        }, timeout);
                    }
                }

                function websiteSearch(term) {
                    var keyword = $(".searchinput").val();
                    if (typeof keyword == "undefined") {
                        keyword = $('.search-field').val();
                    }

                    if (term) {
                        keyword = term;
                    }

                    promo = '/';
                    if (keyword.substring(0, 1) == '#') {
                        promo += 'promo';
                        keyword = keyword.substring(1);
                    }

                    var oldTimestamp = localStorage.getItem('showedPopup');
                    var difference = 86400000; //2629743.83;

                    var currentTimestamp = Date.now();

                    var timeout = 1000;

                    if ((oldTimestamp == null || currentTimestamp - oldTimestamp > difference) && $("#account_type_input").val() == 0) {

                        try {
                            localStorage.setItem('showedPopup', currentTimestamp);
                        } catch (error) {
                            console.log(error);
                        }

                        keyword = serverProcessText(keyword);
                        keyword = keyword.replace("\n", '');

                        var strWord = keyword.replace(/%20/g, "+");
                        var strWord2 = strWord.replace(/ /g, "+");
                        url = 'https://shutterstock.7eer.net/c/51471/43068/1305?u=https://www.shutterstock.com/video/search?searchterm=' + strWord2;

                        if (keyword == 'shutterstock-popular') {
                            url = 'https://shutterstock.7eer.net/c/51471/43068/1305?u=https://www.shutterstock.com/video/search/';
                        }

                        // GA track
                        ga('send', 'event', {
                            eventCategory: 'Shutterstock Redirect',
                            eventAction: 'Opened websiteSearch pagejs',
                            eventLabel: 'url: ' + url
                        });
                        // disable shutterstock popup on search
                        // window.open(url, 'Shutterstock', "height=480,width=640");
                        window.open(url); // Shutterstock Popup in new tab
                        timeout = 1000;
                    }

                    var clip_type = "";
                    var clip_array = {
                        "Footage": "free-stock-footage",
                        "Motion Graphics": "free-motion-graphics",
                        "Music": "royalty-free-music",
                        "Sound Effects": "royalty-free-sound-effects"
                    };
                    if ($(".search-category-filter .category-selector span.text").length) {
                        if (!($(".search-category-filter .category-selector span.text").text().localeCompare("All") == 0))
                            clip_type = '/clip_type/' + clip_array[$(".search-category-filter .category-selector span.text").text()];
                    } else {
                        clip_type = "";
                    }

                    if (!term) {
                        setTimeout(function() {

                            window.location = 'https://videvo.net/search/' + keyword + clip_type + promo;
                        }, timeout);
                    }
                }
                jQuery(document).ready(function($) {

                    $(document).click(function(e) {
                        var target = e.target;

                        if ((!$(target).is('.new-search') && !$(target).parents().is('.new-search')) || ($(target).is('#closeSearch'))) {
                            $('.search-category-filter').css("z-index", "-1");
                            $('.active-search-input').removeClass('active-search-input');
                            $('.active-search-top').removeAttr("style");
                            $('.active-search-top').removeClass('active-search-top');
                            $('.active-searchbtn').removeClass('active-searchbtn');
                            $('#closeSearch').remove();
                        }
                    });

                    $('.menu-icon-resp').click(function() {
                        $(this).toggleClass('open');
                    });

                    $('.search-category-filter .category-selector, .search-category-filter .category-list ul li').click(function(e) {
                        e.preventDefault();
                        toggleSearchMenuHeader($(this));
                    });

                    function toggleSearchMenuHeader(el) {
                        el = el.first();
                        $('.search-category-filter .category-selector a span.text').text(el.text().trim());
                        if ($('.search-category-filter .category-list').css("display") == "none") {
                            $('.search-category-filter .category-list').slideDown(300);
                        } else {
                            $('.search-category-filter .category-list').slideUp(300);
                        }
                        $('.search-category-filter .category-selector a span.icon i').toggleClass('rotate');
                    }

                    $(document)
                        .on('click', '.pre-searchbtn:not(.active-searchbtn)', function(e) {
                            e.preventDefault();

                            var searchinput = document.getElementsByClassName("new-searchinput")[0];
                            searchinput.classList.add("active-search-input");

                            var searchTop = document.getElementById("search-top");
                            searchTop.classList.add("active-search-top");
                            $('.active-search-top').css("width", ($("#menu-headermenu").width() + 45 > 350) ? $("#menu-headermenu").width() + 45 : 350);
                            var searchbtn = document.getElementById("searchbtn");
                            searchbtn.classList.add("active-searchbtn");

                            var close = document.createElement("i");
                            close.className = "fa fa-times fa-2x";
                            close.setAttribute("aria-hidden", "true");
                            close.style.position = "absolute";
                            close.style.right = "0";
                            close.style.bottom = "5px";
                            close.style.color = "white";
                            close.id = "closeSearch";

                            document.getElementsByClassName("search-video")[0].appendChild(close);

                            $('.search-category-filter').css("z-index", "3");

                            searchinput.focus();

                        })

                    $(document)
                        .on('click', '.searchbtn2', function(e) {
                            e.preventDefault();
                            websiteSearch2();
                        })
                        .on('click', '.searchbtn, #searchbtn', function(e) {
                            e.preventDefault();
                            websiteSearch();
                        })
                        .on('click', '#list-videos .pagination a', function() {
                            var __category = '';
                            var __search = '';

                            if (__category || __search) {
                                websiteSearch((__search ? __search : __category));
                            } else {
                                websiteSearch('shutterstock-popular');
                            }
                        });
                    $(".searchinput").keypress(function(event) {
                        if (event.which == 13) {

                            websiteSearch();
                        }
                    });
                    $(".searchinput2").keypress(function(event) {
                        if (event.which == 13) {

                            websiteSearch2();
                        }
                    });

                });
            </script>
